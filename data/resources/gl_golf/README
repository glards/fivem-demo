Vector3 func_539(var uParam0, int iParam1)
{
	if (iParam1 < 0 || iParam1 >= uParam0->f_28)
	{
		return 0f, 0f, 0f;
	}
	switch (iParam1)
	{
		case 0:
			return -1114.121f, 220.789f, 63.78f;
			break;
		
		case 1:
			return -1322.07f, 158.77f, 56.69f;
			break;
		
		case 2:
			return -1237.419f, 112.988f, 56.086f;
			break;
		
